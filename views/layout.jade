!!!
html
  head
    title
      != typeof title == 'undefined' ? 'shareall.co' : title
    meta(name='_csrf', content=csrf_token)
    link(rel='alternate', type='application/rss+xml', title='shareall.co feed', href='/post/rss')
    link(rel='stylesheet', href='/bootstrap.css')
    link(rel='stylesheet', href='/assets/index.styl')
    link(rel='stylesheet', href='/jplayer/skin/blue.monday/jplayer.blue.monday.css')
    link(rel='stylesheet', href='/jquery-ui-1.8.21.custom.css')
    link(rel='stylesheet', href='/jquery-ui-timepicker-addon.css')
    !=js_tag('//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js')
    !=js_tag('//ajax.googleapis.com/ajax/libs/jqueryui/1.8.18/jquery-ui.min.js')
    script
      var express = {};
        with({single_tag_for: function(){
          return express.single_tag_for.apply(this, arguments)
        }, //function (_tag, html_options) { return tag(_tag, html_options, '/>')
        tag: function(){
          return express.tag.apply(this, arguments)
        }
      }){
        !{javascript}
      }
    !=js_tag('/assets/index')
    !=js_tag('/jplayer/jquery.jplayer.js')

  body
    !=partial('_topbar', {selected: typeof selected != 'undefined' && selected})
                
    div(style='background-image: url(/assets/post-bg.png); opacity: 1;position:absolute;top:0;left:0;width:100%;height:40px; z-index: 1')
    div(id='content')
      !=body
